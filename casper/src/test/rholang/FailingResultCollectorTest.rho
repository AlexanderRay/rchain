//scalapackage coop.rchain.rholang

new assert(`rho:test:assertAck`),
    testCompleted(`rho:test:testSuiteCompleted`),
    ack
in {
  assert!("false should fail", false, "clue assertTrue", *ack) |

  for (_ <- ack) {
    assert!("0 != 1 should fail", (0, "==", 1), "clue assertEquals", *ack) |

    for (_ <- ack) {
      testCompleted!(true)
    }
  }
}
